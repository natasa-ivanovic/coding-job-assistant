package sbnz.integracija;

import ftn.sbnz.model.Test;

rule "Add new Test"
    when
        $test: Test (enableMe == false)
    then
        System.out.println("Usao u Test Rule");
        // System.out.println("Ovo je izmena pravila.");
        $test.setTest("Test successful!");
        $test.setEnableMe(true);
        update($test);
end